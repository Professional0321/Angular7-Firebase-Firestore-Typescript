{
  "name": "tanam-angular",
  "version": "0.0.1",
  "license": "Apache-2.0",
  "scripts": {
    "start:admin": "ng serve admin",
    "build:admin": "ng build admin --outputHashing=all --deployUrl=/",
    "build:admin:prod": "ng build admin --prod=true --deployUrl=/",
    "test:admin": "ng test admin",
    "lint:admin": "ng lint admin",
    "e2e:admin": "ng e2e admin",
    "build:core": "ng build tanam-core",
    "start:dynamic": "ng serve dynamic-render --port 4444",
    "build:dynamic": "ng build dynamic-render --outputHashing=all --deployUrl=/",
    "build:dynamic:prod": "ng build dynamic-render --outputHashing=all --aot=false --optimization=true --deployUrl=/",
    "test:dynamic": "ng test dynamic-render",
    "lint:dynamic": "ng lint dynamic-render",
    "e2e:dynamic": "ng e2e dynamic-render",
    "webpack:dynamic": "webpack --config webpack.server.config.js --progress --colors",
    "serve:dynamic:ssr": "node dist/dynamic/server",
    "build:dynamic:ssr": "npm run build:dynamic:bundle && npm run webpack:dynamic",
    "build:dynamic:bundle": "npm run build:dynamic:prod && ng run dynamic-render:server:production",
    "build:all": "npm run build:core && npm run build:admin:prod && npm run build:dynamic:ssr"
  },
  "engines": {
    "node": "8"
  },
  "dependencies": {
    "@angular/animations": "~7.2.2",
    "@angular/cdk": "^7.3.0",
    "@angular/common": "~7.2.2",
    "@angular/compiler": "^7.2.2",
    "@angular/core": "~7.2.2",
    "@angular/fire": "^5.1.1",
    "@angular/forms": "~7.2.2",
    "@angular/http": "~7.2.2",
    "@angular/material": "~7.3.0",
    "@angular/platform-browser": "~7.2.2",
    "@angular/platform-browser-dynamic": "~7.2.2",
    "@angular/platform-server": "~7.2.2",
    "@angular/router": "~7.2.2",
    "@ckeditor/ckeditor5-angular": "^1.0.1",
    "@ckeditor/ckeditor5-build-classic": "^11.2.0",
    "@nguniversal/express-engine": "^7.1.0",
    "@nguniversal/module-map-ngfactory-loader": "7.1.0",
    "core-js": "^2.6.3",
    "express": "^4.16.4",
    "firebase": "^5.8.1",
    "hammerjs": "^2.0.8",
    "rxjs": "~6.3.3",
    "ws": "^6.1.3",
    "xhr2": "^0.1.4",
    "zone.js": "~0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.12.3",
    "@angular-devkit/build-ng-packagr": "~0.12.3",
    "@angular/cli": "~7.2.3",
    "@angular/compiler-cli": "^7.2.2",
    "@angular/language-service": "~7.2.2",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "~10.12.19",
    "bufferutil": "^4.0.1",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.4",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^4.7.0",
    "protractor": "~5.4.2",
    "ts-loader": "^5.3.3",
    "ts-node": "~8.0.2",
    "tsickle": ">=0.34.0",
    "tslib": "^1.9.3",
    "tslint": "~5.12.1",
    "typescript": "~3.2.4",
    "utf-8-validate": "^5.0.2",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1"
  }
}
