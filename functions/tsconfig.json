{
  "compilerOptions": {
    "lib": [
      "es6"
    ],
    "module": "commonjs",
    "noImplicitReturns": true,
    "outDir": "dist",
    "sourceMap": true,
    "target": "es6"
  },
  "compileOnSave": true,
  "typeRoots": [
    "node_modules/@types",
    "../models"
  ],
  "include": [
    "src"
  ],
  "exclude": [
    "../models"
  ]
}
