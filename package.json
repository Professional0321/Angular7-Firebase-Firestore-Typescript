{
  "name": "tanam",
  "version": "0.0.6",
  "description": "Pluggable CMS for Firebase",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "engines": {
    "node": "8"
  },
  "scripts": {
    "watch": "tsc -w -p .",
    "build": "tsc -p .",
    "copyfiles": "cp -r src/cloud_functions examples/minimal/functions/src/tanam/ && cp -r src/template examples/minimal/functions/src/tanam/ && cp -r src/utils examples/minimal/functions/src/tanam/ && cp src/index.ts examples/minimal/functions/src/tanam/",
    "start": "nuxt start",
    "theme:build": "cd src/theme && npm run build",
    "test": "mocha --reporter spec"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oddbit/tanam.git"
  },
  "keywords": [
    "CMS",
    "Firebase"
  ],
  "contributors": [
    {
      "name": "Prabu Rangki",
      "email": "praburangki@gmail.com"
    },
    {
      "name": "Dennis Alund",
      "email": "dennis.alund@gmail.com"
    }
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/oddbit/tanam/issues"
  },
  "homepage": "https://github.com/oddbit/tanam#readme",
  "devDependencies": {
    "@types/crypto-js": "^3.1.43",
    "@types/express": "^4.16.0",
    "@types/handlebars": "^4.0.39",
    "@types/mocha": "^5.2.0",
    "debug": "^3.1.0",
    "mocha": "^5.1.1",
    "tslint": "^5.8.0",
    "typescript": "^2.5.3"
  },
  "dependencies": {
    "crypto-js": "^3.1.9-1",
    "express": "^4.16.3",
    "firebase-admin": "^6.0.0",
    "firebase-functions": "^2.0.5",
    "handlebars": "^4.0.11"
  }
}
